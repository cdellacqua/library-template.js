{
	"name": "lib-template",
	"camelCaseName": "libTemplate",
	"description": "lib template",
	"version": "1.0.0",
	"type": "module",
	"types": "dist/index.d.ts",
	"main": "dist/index.cjs.js",
	"module": "dist/index.es.js",
	"author": "",
	"license": "",
	"keywords": [],
	"repository": {},
	"bugs": {},
	"homepage": "",
	"files": [
		"dist/**/*"
	],
	"exports": {
		".": {
			"import": "./dist/index.es.js",
			"require": "./dist/index.cjs.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"coverage": "c8 npm run test",
		"test": "cross-env mocha 'tests/**/*.ts'",
		"dev:browser": "vite",
		"dev:node": "nodemon",
		"build": "vite build && tsc src/lib/index.ts --declaration --emitDeclarationOnly --declarationDir dist",
		"doc": "rimraf docs && typedoc --readme none --plugin typedoc-plugin-markdown src/lib/index.ts",
		"prepack": "npm run build && npm test && npm run doc",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"prettier": "prettier --check .",
		"prettier:fix": "prettier --write .",
		"setup": "node --unhandled-rejections=strict setup.js"
	},
	"devDependencies": {
		"@types/chai": "^4.2.22",
		"@types/mocha": "^9.0.0",
		"@types/node": "^16.11.10",
		"c8": "^7.10.0",
		"chai": "^4.3.4",
		"cross-env": "^7.0.3",
		"eslint": "^8.3.0",
		"eslint-config-prettier": "^8.3.0",
		"mocha": "^9.1.3",
		"nodemon": "^2.0.15",
		"prettier": "^2.5.0",
		"ts-node": "^10.4.0",
		"typedoc": "^0.22.10",
		"typedoc-plugin-markdown": "^3.11.7",
		"typescript": "^4.3.2",
		"vite": "^2.6.4"
	}
}
